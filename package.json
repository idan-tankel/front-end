{
  "name": "universaldot-frontend",
  "author": "universaldot-foundation",
  "version": "0.1.0",
  "description": "UniversalDot Frontend",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "serve": "serve --single build",
    "eject": "react-scripts eject",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
